body
  font 12px Helvetica, Arial, sans-serif
  padding 0px
  margin 0px
  background-color #eee


#app
  display flex
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: stretch;
  align-content: flex-end;
  align-items: flex-start;
  label
   margin-top 10px
   
.border
  background-color: #ddd
  border-radius: 5px
  margin 1em
  padding 1em
  border 1px solid black
.inspector
  max-width 300px
  img
    width 100%
.flex
  flex 1 1 100
  min-width: 100px
  min-height: 10px
  display flex
  flex-direction column
  .links
    border none
    flex-direction row
    margin 0em
    padding 0.5em
    padding-left 0em
    a
      padding-right 1em
.row
  flex-direction row

.interactive-container
  max-width: 600px
  display: inline-block
  padding: 10px
  margin-right: -10px
  input
    width: 100%
    box-sizing: border-box

button
  margin 0.2em
label
  font-weight bold
  margin 0.2em
textArea
  min-width 200px
  min-height 100px
  width 100%
.field-row
  min-height: 10px
  margin: 0px
  border: none
.bottomTools
  min-width 90%
  display flex
  flex 100 0 1000
  flex-direction row
  flex-wrap nowrap
  justify-content stretch
  align-self flex-end
  align-content stetch
  align-items space-between
    
div#logger
  width 100%
  height 100px
  background-color #fff
  font-family monospace
  overflow auto
  display inline
  border none
  line-height 1.4em
  span.logmsg
    -moz-animation-duration 1s
    -webkit-animation-duration 1s
    animation-duration 1s
    
    animation-name fadeout
    -webkit-animation-name fadeout
    -moz-animation-name fadeout
      
      
    
